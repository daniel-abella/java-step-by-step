import java.sql.Date;
import java.util.ArrayList;
import java.util.List;


public class ExtendsSuper {

	public static void main(String[] args) {

		somaExtends(new ArrayList<Integer>());
		somaExtends(new ArrayList<Long>());
		somaExtends(new ArrayList<Short>());
	}

	public static void somaExtends(List<? extends Number> valor){

		valor.add(1);
	}

	public static void somaSuper(List<? extends Date> valor){

	}

}
