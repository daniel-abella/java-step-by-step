import java.io.BufferedReader;
import java.io.File;
import java.io.IOException;

class Boxing3{

  public static void main(String args[]) throws IOException{
	  for (int index = 1; index <= 2; index++) {
		  PrintWriter writer = new PrintWriter("/apa");
		  writer.print("apa");
		  writer.close();
 }
}